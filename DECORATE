// Ambience Sounds

Actor Ambience_Tech_Outdoors 14002
{
+THRUACTORS
Radius 4
Height 4
States
{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_PlaySoundEx("TECHAMB", "Auto", 1)
		Goto Live
		
	Live:
		TNT1 A 24
		Loop
	}
}

