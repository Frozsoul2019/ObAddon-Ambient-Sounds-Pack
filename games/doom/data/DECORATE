// Ambience Sounds

Actor Ambience_Tech 14001
{
+THRUACTORS
Radius 4
Height 4
States
{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_PlaySoundEx("TECHAMB", 1, 1)
		Goto Live
		
	Live:
		TNT1 A 24
		Loop
	}
}

